---
import EditorLayout from '@layouts/EditorLayout.astro'
import { getLinks } from '@api/index'

const links = await getLinks()

const current_tab = {
  name: 'NotFound',
  icon: 'ooui:article-not-found-ltr'
}

const tabs = [
  current_tab
]
---

<EditorLayout current_tab={current_tab} extension="404" links={links} tabs={tabs}>
  <div class="flex flex-col items-center justify-center m-4 gap-y-4">
    <iconify-icon icon="iconoir:file-not-found" class="text-slate-400/20" width="128" height="128" />
    <div class="text-center text-slate-400/50">It appears that you are lost<br />because there's nothing here ðŸ˜¢
    </div>
  </div>
</EditorLayout>