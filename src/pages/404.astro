---
import EditorLayout from "@layouts/EditorLayout.astro"
import { links } from "@data/links.json"

const { href, origin } = Astro.url

const tabs = [
  {
    name: "NotFound",
    icon: "ooui:article-not-found-ltr"
  }
]

const current_tab = {
  name: "NotFound"
}
---

<EditorLayout current_tab={current_tab} extension="404" links={links} tabs={tabs}>
  <div class="flex flex-col gap-y-4 items-center justify-center">
    <div class="i-iconoir:file-not-found w-32 h-32 text-slate-400/20"></div>
    <div class="text-slate-400/50 text-center">It appears that you are lost<br />because I don't know
      <code>{href.replace(origin, '')}</code> ðŸ˜¢
    </div>
  </div>
</EditorLayout>